# Unique name to use with CloudFormation [a-zA-Z][-a-zA-Z0-9]*
export STACK_NAME=ANY_UNIQUE_NAME
# 221 is currently the only supported version
export CF_RELEASE=221
# AWS credentials
export AWS_ACCESS_KEY_ID="YOUR_KEY"
export AWS_SECRET_ACCESS_KEY="YOUR_SECRET"
export AWS_DEFAULT_REGION="us-east-1"
# a password for CF and BOSH components and for their admin accounts
export SECRET="YOUR_SECRET"
